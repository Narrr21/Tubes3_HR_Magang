/* Main Window Styling */
QMainWindow {
    background: qlineargradient(x1:0, y1:0, x2:1, y2:1, stop:0 #f8fafc, stop:1 #e2e8f0);
}

/* Title Label */
QLabel#lblTitle {
    background: qlineargradient(x1:0, y1:0, x2:1, y2:0, stop:0 #4f46e5, stop:1 #7c3aed);
    color: white;
    padding: 15px;
    border-radius: 12px;
    font-weight: bold;
    font-size: 20px;
}

/* Group Box Styling */
QGroupBox {
    font-weight: bold;
    font-size: 14px;
    color: #374151;
    border: 2px solid #e5e7eb;
    border-radius: 12px;
    margin-top: 10px;
    padding-top: 15px;
    background: white;
}

QGroupBox::title {
    subcontrol-origin: margin;
    left: 15px;
    padding: 5px 15px;
    background: qlineargradient(x1:0, y1:0, x2:1, y2:0, stop:0 #4f46e5, stop:1 #7c3aed);
    color: white;
    border-radius: 8px;
    font-size: 13px;
}

/* Upload Group Styling */
QGroupBox#groupUpload {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #fef7ff, stop:1 #f3e8ff);
    border: 2px solid #c084fc;
}

/* Search Group Styling */
QGroupBox#groupSearch {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #eff6ff, stop:1 #dbeafe);
    border: 2px solid #60a5fa;
}

/* Database Group Styling */
QGroupBox#groupDatabaseInfo {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #f0fdf4, stop:1 #dcfce7);
    border: 2px solid #4ade80;
}

/* Results Group Styling */
QGroupBox#groupResults {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #fefce8, stop:1 #fef3c7);
    border: 2px solid #fbbf24;
}

/* Button Styling */
QPushButton {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #6366f1, stop:1 #4f46e5);
    color: white;
    border: none;
    border-radius: 8px;
    padding: 8px 16px;
    font-weight: bold;
    font-size: 12px;
}

QPushButton:hover {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #5855eb, stop:1 #4338ca);
}

QPushButton:pressed {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #4338ca, stop:1 #3730a3);
}

QPushButton:disabled {
    background: #9ca3af;
    color: #6b7280;
}

/* Browse Button */
QPushButton#btnBrowse {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #10b981, stop:1 #059669);
}

QPushButton#btnBrowse:hover {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #059669, stop:1 #047857);
}

/* Upload Button */
QPushButton#btnUpload {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #8b5cf6, stop:1 #7c3aed);
}

QPushButton#btnUpload:hover {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #7c3aed, stop:1 #6d28d9);
}

/* Clear Button */
QPushButton#btnClear {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #ef4444, stop:1 #dc2626);
}

QPushButton#btnClear:hover {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #dc2626, stop:1 #b91c1c);
}

/* Input Fields */
QLineEdit {
    border: 2px solid #e5e7eb;
    border-radius: 8px;
    padding: 8px 12px;
    font-size: 13px;
    background: white;
}

QLineEdit:focus {
    border-color: #6366f1;
    outline: none;
}

QLineEdit:read-only {
    background: #f9fafb;
    color: #6b7280;
}

/* Spin Box */
QSpinBox {
    border: 2px solid #e5e7eb;
    border-radius: 8px;
    padding: 5px;
    background: white;
    font-size: 13px;
}

QSpinBox:focus {
    border-color: #6366f1;
}

/* Radio Buttons */
QRadioButton {
    font-size: 13px;
    color: #374151;
    spacing: 8px;
}

QRadioButton::indicator {
    width: 18px;
    height: 18px;
}

QRadioButton::indicator:unchecked {
    border: 2px solid #d1d5db;
    border-radius: 9px;
    background: white;
}

QRadioButton::indicator:checked {
    border: 2px solid #6366f1;
    border-radius: 9px;
    background: qlineargradient(x1:0, y1:0, x2:1, y2:1, stop:0 #6366f1, stop:1 #4f46e5);
}

/* List Widgets */
QListWidget {
    border: 2px solid #e5e7eb;
    border-radius: 8px;
    background: white;
    alternate-background-color: #f9fafb;
    selection-background-color: #6366f1;
    selection-color: white;
    font-size: 13px;
}

QListWidget::item {
    padding: 8px;
    border-bottom: 1px solid #f3f4f6;
}

QListWidget::item:hover {
    background: #f3f4f6;
}

QListWidget::item:selected {
    background: qlineargradient(x1:0, y1:0, x2:1, y2:0, stop:0 #6366f1, stop:1 #4f46e5);
}

/* Labels */
QLabel {
    color: #374151;
    font-size: 13px;
}

QLabel#lblPerformanceTitle {
    color: #059669;
    font-weight: bold;
    font-size: 14px;
}

/* Menu Bar */
QMenuBar {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #1f2937, stop:1 #111827);
    color: white;
    border: none;
    padding: 2px;
}

QMenuBar::item {
    background: transparent;
    padding: 8px 16px;
    border-radius: 4px;
    margin: 2px;
}

QMenuBar::item:selected {
    background: #374151;
}

QMenu {
    background: white;
    border: 1px solid #e5e7eb;
    border-radius: 8px;
    padding: 5px;
}

QMenu::item {
    padding: 8px 20px;
    border-radius: 4px;
}

QMenu::item:selected {
    background: #f3f4f6;
}

/* Status Bar */
QStatusBar {
    background: #f9fafb;
    border-top: 1px solid #e5e7eb;
    color: #6b7280;
}
